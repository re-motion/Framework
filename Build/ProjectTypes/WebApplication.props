    <PackageReference Include="CoreForms.Web.Infrastructure" />
    <PackageReference Include="Microsoft.Net.Compilers.Toolset" VersionOverride="3.10.0" ExcludeAssets="all" GeneratePathProperty="true" />
  </ItemGroup>

</Project>