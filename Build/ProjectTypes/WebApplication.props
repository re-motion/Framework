<Project>

  <Import Project="WebLibrary.props" />

  <PropertyGroup>
    <UseWebForms>true</UseWebForms>
    <TargetFrameworks>net8.0-windows</TargetFrameworks>
    <OutputPath>bin</OutputPath>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <IsPackable>false</IsPackable>
    <Nullable>disable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CoreForms.Web.Infrastructure.MachineConfig" />
    <PackageReference Include="CoreForms.Web.Infrastructure.Roslyn" />
    <PackageReference Include="CoreForms.Web.Infrastructure" />
    <PackageReference Include="Microsoft.Net.Compilers.Toolset" ExcludeAssets="all" GeneratePathProperty="true" />
  </ItemGroup>

</Project>