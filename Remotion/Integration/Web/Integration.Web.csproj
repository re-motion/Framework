<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <ProjectType>Library</ProjectType>
  </PropertyGroup>
  <Import Project="..\..\..\Build\Shared.build.props" />
  <PropertyGroup>
    <AssemblyTitle>re-motion Integration Library for Web-Applications</AssemblyTitle>
    <Description>This assembly can be linked to ensure that all web-related dependencies are integrated in the build output of a web project.</Description>
    <AssemblyName>Remotion.Integration.Web</AssemblyName>
    <RootNamespace>$(AssemblyName)</RootNamespace>
    <TargetFrameworks>net8.0-windows</TargetFrameworks>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\ObjectBinding\Web\ObjectBinding.Web.csproj" />
    <ProjectReference Include="..\..\Web\Core\Web.Core.csproj" />
    <ProjectReference Include="..\..\Web\Security\Web.Security.csproj" />
    <ProjectReference Include="..\Domain\Integration.Domain.csproj" />
  </ItemGroup>
  <Import Project="..\..\..\Build\Shared.build.targets" />
</Project>