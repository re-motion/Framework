<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <ProjectType>Library</ProjectType>
  </PropertyGroup>
  <Import Project="..\..\..\Build\Shared.build.props" />
  <PropertyGroup>
    <AssemblyTitle>re-motion log4net-based Logging Library</AssemblyTitle>
    <Description>Provides an implementation of the Microsoft ILogger API based on log4net.</Description>
    <AssemblyName>Remotion.Logging.Log4Net</AssemblyName>
    <RootNamespace>$(AssemblyName)</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Remove="JetBrains.Annotations" />
    <PackageReference Include="log4net" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" />
  </ItemGroup>
  <Import Project="..\..\..\Build\Shared.build.targets" />
</Project>