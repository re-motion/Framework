      <controls>
        <add tagPrefix="asp" namespace="System.Web.UI.WebControls.WebParts" assembly="CoreForms.Web"/>
      </controls>
    </pages>
    <processModel autoConfig="true"/>
    <urlMappings enabled="true"/>
    <webControls clientScriptsLocation="/aspnet_client/{0}/{1}/"/>
