<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <!-- Default configuration for .NET Core applications -->
    <section name="appSettings" type="System.Configuration.AppSettingsSection, System.Configuration.ConfigurationManager" restartOnExternalChanges="false" requirePermission="false" />
    <section name="connectionStrings" type="System.Configuration.ConnectionStringsSection, System.Configuration.ConfigurationManager" requirePermission="false" />
    <section name="mscorlib" type="System.Configuration.IgnoreSection, System.Configuration.ConfigurationManager" allowLocation="false" />
    <section name="runtime" type="System.Configuration.IgnoreSection, System.Configuration.ConfigurationManager" allowLocation="false" />
    <section name="assemblyBinding" type="System.Configuration.IgnoreSection, System.Configuration.ConfigurationManager" allowLocation="false" />
    <section name="satelliteassemblies" type="System.Configuration.IgnoreSection, System.Configuration.ConfigurationManager" allowLocation="false" />
    <section name="startup" type="System.Configuration.IgnoreSection, System.Configuration.ConfigurationManager" allowLocation="false" />

    <section name="system.codedom" type="System.CodeDom.Compiler.CodeDomConfigurationHandler, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"/>
    <sectionGroup name="system.web" type="System.Web.Configuration.SystemWebSectionGroup, CoreForms.Web">
      <section name="anonymousIdentification" type="System.Web.Configuration.AnonymousIdentificationSection, CoreForms.Web"
               allowDefinition="MachineToApplication" />
      <section name="authentication" type="System.Web.Configuration.AuthenticationSection, CoreForms.Web" allowDefinition="MachineToApplication" />
      <section name="authorization" type="System.Web.Configuration.AuthorizationSection, CoreForms.Web" />
      <section name="browserCaps" type="System.Web.Configuration.HttpCapabilitiesSectionHandler, CoreForms.Web" />
      <section name="clientTarget" type="System.Web.Configuration.ClientTargetSection, CoreForms.Web" />
      <section name="compilation" type="System.Web.Configuration.CompilationSection, CoreForms.Web" requirePermission="false" />
      <section name="customErrors" type="System.Web.Configuration.CustomErrorsSection, CoreForms.Web" />
      <section name="deployment" type="System.Web.Configuration.DeploymentSection, CoreForms.Web" allowDefinition="MachineOnly" />
      <section name="fullTrustAssemblies" type="System.Web.Configuration.FullTrustAssembliesSection, CoreForms.Web"
               allowDefinition="MachineToApplication" />
      <section name="globalization" type="System.Web.Configuration.GlobalizationSection, CoreForms.Web" />
      <section name="healthMonitoring" type="System.Web.Configuration.HealthMonitoringSection, CoreForms.Web"
               allowDefinition="MachineToApplication" />
      <section name="hostingEnvironment" type="System.Web.Configuration.HostingEnvironmentSection, CoreForms.Web"
               allowDefinition="MachineToApplication" />
      <section name="httpCookies" type="System.Web.Configuration.HttpCookiesSection, CoreForms.Web" />
      <section name="httpHandlers" type="System.Web.Configuration.HttpHandlersSection, CoreForms.Web" />
      <section name="httpModules" type="System.Web.Configuration.HttpModulesSection, CoreForms.Web" />
      <section name="httpRuntime" type="System.Web.Configuration.HttpRuntimeSection, CoreForms.Web" />
      <section name="identity" type="System.Web.Configuration.IdentitySection, CoreForms.Web" />
      <section name="machineKey" type="System.Web.Configuration.MachineKeySection, CoreForms.Web" allowDefinition="MachineToApplication" />
      <section name="membership" type="System.Web.Configuration.MembershipSection, CoreForms.Web" allowDefinition="MachineToApplication" />
      <section name="pages" type="System.Web.Configuration.PagesSection, CoreForms.Web" requirePermission="false" />
      <section name="partialTrustVisibleAssemblies" type="System.Web.Configuration.PartialTrustVisibleAssembliesSection, CoreForms.Web"
               allowDefinition="MachineToApplication" />
      <section name="processModel" type="System.Web.Configuration.ProcessModelSection, CoreForms.Web" allowDefinition="MachineOnly"
               allowLocation="false" />
      <section name="profile" type="System.Web.Configuration.ProfileSection, CoreForms.Web" allowDefinition="MachineToApplication" />
      <section name="protocols" type="System.Web.Configuration.ProtocolsSection, CoreForms.Web" allowDefinition="MachineToWebRoot" />
      <section name="roleManager" type="System.Web.Configuration.RoleManagerSection, CoreForms.Web" allowDefinition="MachineToApplication" />
      <section name="securityPolicy" type="System.Web.Configuration.SecurityPolicySection, CoreForms.Web" allowDefinition="MachineToApplication" />
      <section name="sessionPageState" type="System.Web.Configuration.SessionPageStateSection, CoreForms.Web" />
      <section name="sessionState" type="System.Web.Configuration.SessionStateSection, CoreForms.Web" allowDefinition="MachineToApplication" />
      <section name="siteMap" type="System.Web.Configuration.SiteMapSection, CoreForms.Web" allowDefinition="MachineToApplication" />
      <section name="trace" type="System.Web.Configuration.TraceSection, CoreForms.Web" />
      <section name="trust" type="System.Web.Configuration.TrustSection, CoreForms.Web" allowDefinition="MachineToApplication" />
      <section name="urlMappings" type="System.Web.Configuration.UrlMappingsSection, CoreForms.Web" allowDefinition="MachineToApplication" />
      <section name="webControls" type="System.Web.Configuration.WebControlsSection, CoreForms.Web" />
      <section name="webParts" type="System.Web.Configuration.WebPartsSection, CoreForms.Web" />
      <section name="webServices" type="System.Web.Services.Configuration.WebServicesSection, CoreForms.Web.Services"/>
      <section name="xhtmlConformance" type="System.Web.Configuration.XhtmlConformanceSection, CoreForms.Web" />
      <sectionGroup name="caching" type="System.Web.Configuration.SystemWebCachingSectionGroup, CoreForms.Web">
        <section name="cache" type="System.Web.Configuration.CacheSection, CoreForms.Web" allowDefinition="MachineToApplication" />
        <section name="outputCache" type="System.Web.Configuration.OutputCacheSection, CoreForms.Web" allowDefinition="MachineToApplication" />
        <section name="outputCacheSettings" type="System.Web.Configuration.OutputCacheSettingsSection, CoreForms.Web"
                 allowDefinition="MachineToApplication" />
        <section name="sqlCacheDependency" type="System.Web.Configuration.SqlCacheDependencySection, CoreForms.Web"
                 allowDefinition="MachineToApplication" />
      </sectionGroup>
    </sectionGroup>
  </configSections>
  <system.web>
    <authentication>
      <forms cookieless="UseCookies"></forms>
    </authentication>
    <authorization>
      <allow users="*"/>
    </authorization>
    <browserCaps userAgentCacheKeyLength="64">
      <result type="System.Web.HttpBrowserCapabilities, CoreForms.Web"/>
    </browserCaps>
    <clientTarget>
      <add alias="uplevel" userAgent="Mozilla/5.0 (compatible; MSIE 6.0; Windows NT 5.1)"/>
      <add alias="downlevel" userAgent="Generic Downlevel"/>
    </clientTarget>
    <compilation debug="true" targetFramework="5.0">
      <assemblies>
        <add assembly="mscorlib" />
        <add assembly="netstandard" />
        <add assembly="System.CodeDom" />
        <add assembly="System.Collections" />
        <add assembly="System.Collections.Concurrent" />
        <add assembly="System.Collections.NonGeneric" />
        <add assembly="System.Collections.Specialized" />
        <add assembly="System.ComponentModel" />
        <add assembly="System.Configuration.ConfigurationManager" />
        <add assembly="System.Data" />
        <add assembly="System.Data.Common" />
        <add assembly="System.Diagnostics.FileVersionInfo" />
        <add assembly="System.Diagnostics.StackTrace" />
        <add assembly="System.Diagnostics.TextWriterTraceListener" />
        <add assembly="System.Diagnostics.TraceSource" />
        <add assembly="System.Diagnostics.Tools"/>
        <add assembly="System.IO.Compression" />
        <add assembly="System.IO.FileSystem.Watcher" />
        <add assembly="System.Net.HttpListener" />
