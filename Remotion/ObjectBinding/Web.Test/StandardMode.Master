<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="StandardMode.master.cs"
  Inherits="OBWTest.StandardMode" %>
<%@ Import Namespace="System" %>

<!DOCTYPE html>
<html lang="en" ID="Html" runat="server">
<head runat="server">
  <meta http-equiv="X-UA-Compatible" content="IE=Edge">
  <meta http-equiv="content-type" content="text/html; charset=UTF-8" />
  <title runat="server" />
  <remotion:HtmlHeadContents ID="HtmlHeadContents" runat="server" />
  <asp:ContentPlaceHolder ID="head" runat="server">
  </asp:ContentPlaceHolder>
  <style type="text/css">
    html
    {
      height: 100%;
      margin: 0;
      padding: 0;
      overflow: clip;
    }
    body
    {
      height: 100%;
      margin: 0;
      padding: 0;
      overflow: clip;
    }
    form
    {
      height: 100%;
      width: 100%;
    }
    html.scrollable
    {
      overflow: scroll;
    }
    html.scrollable>body
    {
      overflow: visible;
    }
    html.scrollable #body_SingleView
    {
      min-width: 1000px;
      min-height: 800px;
    }
  </style>
</head>
<body>
  <form id="form1" runat="server">
  <div style="height: 100%">
    <asp:ContentPlaceHolder ID="body" runat="server">
    </asp:ContentPlaceHolder>
  </div>
  </form>
</body>
</html>
